generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  Notes    Notes[]
  Wifis    Wifis[]
}

model Notes {
  id      Int    @id @default(autoincrement())
  user_id Int
  note    String
  tittle  String
  Users   Users  @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model Wifis {
  id               Int    @id @default(autoincrement())
  user_id          Int
  network_name     String
  network_password String
  tittle           String
  Users            Users  @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
